{
    "name": "tomslominski/uptime-monitor",
    "type": "project",
    "description": "Wrapper for Spatie's Laravel Uptime Monitor.",
    "keywords": [
        "laravel"
    ],
    "license": "ISC",
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.4",
        "javoscript/laravel-macroable-models": "^1.0",
        "laravel-notification-channels/pushover": "^3.1.0",
        "laravel/framework": "^9.0",
        "laravel/tinker": "^2.0",
        "spatie/laravel-uptime-monitor": "^4.2.1",
        "symfony/http-client": "^6.0",
        "symfony/mailgun-mailer": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0",
        "spatie/laravel-ignition": "^1.0",
        "spatie/laravel-ray": "^1.29",
        "spatie/ray": "^1.37"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
